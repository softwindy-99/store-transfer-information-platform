/**index.wxss**/
/* 顶部导航搜索栏 */
.Navigation_search {
  width: 730rpx;
  height: 70rpx;
  position: absolute;
  top: 0rpx;
  margin: 5rpx 10rpx;
  border-radius: 50rpx;
  background-color: rgba(255, 255, 255,1);
  border: solid 1rpx rgb(20,150,220);
  z-index: 8;
}
.Navigation_search_picker {
  width: 110rpx;
  line-height: 70rpx;
  font-size: 23rpx;
  padding-left: 10rpx;
  text-align: center;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  background-color: rgba(20,150,220,0.2);
}
.Navigation_search picker {
  float: left;
}
.Navigation_search input {
  width: 470rpx;
  height: 70rpx;
  padding: 0rpx 10rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  float: left;
}
.Navigation_search button {
  width: 120rpx!important;
  height: 70rpx!important;
  border-radius: 50rpx!important;
  background-color: rgb(20,150,220)!important;
  font-size: 30rpx;
  line-height: 70rpx;
  padding: 0rpx 0rpx;
  float: left;
}
.Navigation_search button::after{
 border: none;
}

/* 轮播图栏 */
.Swiper_bar {
  width: 100%;
  height: 400rpx;
  box-shadow: 0px 0px 2px 1px rgb(220,220,220);
  margin-bottom: 30rpx;
}
.Swiper_bar_item image {
  width: 100%;
  height: 400rpx;
}

/* 发布信息栏 */
.Release_information {
  width: 100%;
  height: 360rpx;
  margin: 20rpx 0rpx 20rpx 0rpx;
  padding: 40rpx 0rpx 40rpx 0rpx;
  box-shadow: 0px 0px 5px 1px rgb(220,220,220);
  background-color: rgb(255,255,255);
}
.Release_information_shop {
  width: 346rpx;
  height: 340rpx;
  margin: 10rpx 5rpx 10rpx 20rpx ;
  border: solid 2rpx rgb(20,150,220);
  border-radius: 10rpx;
  float: left;
  background-color: rgb(255,255,255);
}
.Release_information_shop_button {
  width: 330rpx;
  height: 140rpx;
  margin: 10rpx 8rpx 20rpx 8rpx;
  border-radius: 15rpx;
  background-color: rgb(20,150,220);
  color: rgb(255,255,255);
  font-size: 35rpx;
  font-weight: 500;
  text-align: center;
  line-height: 140rpx;
  position: relative;
}
.Release_information_shop_button_icon {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  bottom: 0rpx;
  right: 0rpx;
}
.Release_information_shop_button_icon image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 0rpx;
}
.Release_information_shop_text view {
  width: auto;
  height: 35rpx;
  margin: 40rpx 0rpx 0rpx 20rpx;
  font-size: 32rpx;
  color: rgb(20,150,220);
}
.Release_information_rent {
  width: 346rpx;
  height: 340rpx;
  margin: 10rpx 20rpx 10rpx 5rpx;
  border: solid 2rpx rgb(20,150,220);
  border-radius: 10rpx;
  float: left;
  background-color: rgb(255,255,255);
}
.Release_information_rent_button {
  width: 330rpx;
  height: 140rpx;
  margin: 10rpx 8rpx 20rpx 8rpx;
  border-radius: 15rpx;
  background-color: rgb(20,180,230);
  color: rgb(255,255,255);
  font-size: 35rpx;
  font-weight: 500;
  text-align: center;
  line-height: 140rpx;
  position: relative;
}
.Release_information_rent_button_icon {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  bottom: 0rpx;
  right: 0rpx;
}
.Release_information_rent_button_icon image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 0rpx;
}
.Release_information_rent_text view {
  width: auto;
  height: 35rpx;
  margin: 40rpx 0rpx 0rpx 20rpx;
  font-size: 32rpx;
  color: rgb(20,180,230);
}
/* 推广列表 */
.Advertisement_information {
  width: 100%;
  height: auto;
  margin: 30rpx 0rpx 30rpx 0rpx;
  padding: 20rpx 0rpx 60rpx 0rpx;
  background-color: rgb(255,255,255);
  box-shadow: 0px 0px 5px 1px rgb(220,220,220);
}
.Advertisement_information_title {
  width: 730rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 50rpx;
  font-weight: 500;
  color: rgb(20,150,220);
  margin-left: 20rpx;
}
.Advertisement_information_tab {
  width: 100%;
  height: 80rpx;
  margin: 10rpx 0rpx 0rpx 0rpx;
}
.Advertisement_information_tab view {
  width: auto;
  height: 80rpx;
  padding: 0rpx 20rpx 0rpx 20rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  float: left;
}
.Advertisement_information_picker {
  width: 100%;
  height: 80rpx;
}
.Advertisement_information_picker_item {
  width: 187.5rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: rgba(0, 0, 0, 0.5);
  float: left;
}
.Advertisement_shop_list {
  width: 100%;
  height: auto;
  padding: 20rpx 0rpx 20rpx 0rpx;
  background-color: rgba(255,255,255,1);
}
.Advertisement_shop_list_item {
  width: 700rpx;
  height: 200rpx;
  margin: 10rpx 25rpx 10rpx 25rpx;
  border-radius: 20rpx;
  border: solid 1px rgb(220,220,220);
  box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.2);
  background-color: rgb(255,255,255);
}
.Advertisement_shop_list_item_image {
  width: 260rpx;
  height: 160rpx;
  margin: 20rpx 20rpx 20rpx 20rpx;
  border-radius: 10rpx;
  float: left;
}
.Advertisement_shop_list_item_image image {
  width: 260rpx;
  height: 160rpx;
  border-radius: 10rpx;
}
.Advertisement_shop_list_item_text {
  width: 400rpx;
  height: 100%;
  float: left;
}
.Advertisement_shop_list_item_text_title {
  width: 400rpx;
  height: 70rpx;
  font-size: 32rpx;
  line-height: 70rpx;
  text-align: center;
  overflow: hidden;
}
.Advertisement_shop_list_item_text_area {
  width: 380rpx;
  height: auto;
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.5);
  margin: 10rpx 0rpx 10rpx 0rpx;
  padding: 0rpx 0rpx 0rpx 0rpx;
}
.Advertisement_shop_list_item_text_monthly_rent {
  width: auto;
  height: auto;
  font-size: 22rpx;
  color: rgba(0, 0, 0, 0.5);
  margin: 10rpx 0rpx 10rpx 0rpx;
  padding: 0rpx 0rpx 0rpx 0rpx;
  float: left;
}
.Advertisement_shop_list_item_text_transfer_fee {
  width: auto;
  height: auto;
  font-size: 22rpx;
  color: rgba(0, 0, 0, 0.5);
  margin: 10rpx 0rpx 10rpx 0rpx;
  padding: 0rpx 0rpx 0rpx 10rpx;
  float: left;
}
.Advertisement_rent_list {
  width: 100%;
  height: auto;
  padding: 20rpx 0rpx 20rpx 0rpx;
  background-color: rgb(255,255,255);
}
.Advertisement_rent_list_item {
  width: 700rpx;
  height: 200rpx;
  margin: 10rpx 25rpx 10rpx 25rpx;
  border-radius: 20rpx;
  border: solid 1px rgb(220,220,220);
  box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.2);
  background-color: rgb(255,255,255);
}
.Advertisement_rent_list_item_text {
  width: 475rpx;
  height: 100%;
  float: left;
}
.Advertisement_rent_list_item_title {
  width: auto;
  height: auto;
  font-size: 36rpx;
  padding: 10rpx 0rpx 10rpx 10rpx;
}
.Advertisement_rent_list_item_other {
  width: auto;
  height: auto;
  font-size: 32rpx;
  color: rgba(0, 0, 0, 0.5);
  padding: 10rpx 0rpx 10rpx 10rpx;
  word-wrap: break-word;
}
.Advertisement_rent_list_item_area {
  width: 225rpx;
  height: auto;
  font-size: 26rpx;
  line-height: 200rpx;
  text-align: center;
  color: rgb(230,80,60);
  float: left;
}
/* 底部导航栏 */
.Navigation_bar {
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0rpx;
  background-color: rgb(255,255,255);
  padding-top: 10rpx;
  padding-bottom: 20rpx;
  z-index: 10;
}
.Navigation_bar_item {
  width: 250rpx;
  height: 120rpx;
  float: left;
}
.Navigation_bar_item image {
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
  display: block;
}
.Navigation_bar_item text {
  text-align: center;
  display: block;
}